{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "convert",
  "version": "0.1.0",
  "identifier": "com.convert-app.app",
  "bundle": {
    "resources":[
      "assets/win/*",
      "assets/win/**/*"
    ],
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": null,
      "digestAlgorithm": null,
      "nsis": null,
      "signCommand": null,
      "timestampUrl": null,
      "tsp": false,
      "webviewInstallMode": {
        "silent": true,
        "type": "offlineInstaller"
      },
      "wix": null
    }
  }
}
